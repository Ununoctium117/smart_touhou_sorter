{
  "name": "web_ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "run-s wasm-pack:release svelte:check vite:build",
    "dev": "run-s wasm-pack:dev vite:dev",
    "vite:dev": "vite -c vite.config.dev.ts",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "wasm-pack:dev": "wasm-pack build --dev ../web-engine",
    "wasm-pack:release": "wasm-pack build ../web-engine",
    "svelte:check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^24.10.1",
    "npm-run-all": "^4.1.5",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.5",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vite-plugin-wasm-pack-watcher": "^1.0.1"
  },
  "optionalDependencies": {
    "esbuild": "^0.27.2"
  },
  "resolutions": {
    "vite": "npm:rolldown-vite@7.2.5",
    "rollup": "npm:@rollup/wasm-node@*"
  },
  "dependencies": {
    "web-engine": "link:../web-engine/pkg"
  }
}
